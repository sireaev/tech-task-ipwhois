{
  "name": "tech-task-ipwhois",
  "version": "0.0.0",
  "description": "REST API lookup ipwhois for an IP, caching DB",
  "scripts": {
    "preinstall": "pnpm exec only-allow pnpm",
    "start": "nodemon --exec babel-node --extensions \".ts\" ./src/server.ts",
    "build": "babel src --out-dir dist --extensions \".ts\",\".js\"",
    "serve": "NODE_ENV=production node ./dist/server.js",
    "lint": "pnpm exec eslint ./src/**/* --ext .ts --quiet --fix",
    "db:create": "pnpm exec sequelize-cli db:create",
    "db:migrate": "pnpm exec sequelize-cli db:migrate",
    "db:seed": "pnpm exec sequelize-cli db:seed:all",
    "db:drop": "pnpm exec sequelize-cli db:drop",
    "db:reset": "pnpm run db:drop && pnpm exec sequelize-cli db:create && pnpm run db:migrate && pnpm run db:seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "helmet": "^7.0.0",
    "http-errors": "^2.0.0",
    "mysql2": "^3.6.1",
    "node-cron": "^3.0.2",
    "sequelize": "^6.33.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/node": "^7.22.19",
    "@babel/plugin-transform-class-properties": "^7.22.5",
    "@babel/plugin-transform-object-rest-spread": "^7.22.15",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.23.0",
    "@babel/register": "^7.22.15",
    "@babel/runtime": "^7.23.1",
    "@types/body-parser": "^1.19.3",
    "@types/cookie-parser": "^1.4.4",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/http-errors": "^2.0.2",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@types/node-cron": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "sequelize-cli": "6.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.x",
    "pnpm": ">=3",
    "sequelize-cli": ">=6.6.x"
  }
}
